{
	"extends": "next/core-web-vitals",
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/consistent-type-imports": "error",
		"import/order": [
			"error",
			{
				"groups": [
					["builtin", "external"],
					"internal",
					"parent",
					"sibling",
					"index"
				],
				"newlines-between": "always",
				"alphabetize": {
					"order": "asc",
					"caseInsensitive": true
				}
			}
		]
	}
}
